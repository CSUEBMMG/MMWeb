!!!
- page_title = (current_page.data.title.is_a?(String) ? "#{current_page.data.title} - " : '') + "Cal State East Bay Multimedia Graduate Program (MM Grad)"
%html(lang="#{current_page.data.lang || "en"}")
  %head
    %meta(charset="utf-8")
    %meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1")
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    %meta(content="IE=edge,chrome=1" http-equiv="X-UA-Compatible")
    %meta(name="x-unique-key" content="#{current_page.data.unique_key ? current_page.data.unique_key : current_page.slug}")
    %link(rel="canonical" href="#{data.config.canonical_host}#{current_page.url}")
    %title= page_title
    = stylesheet_link_tag "all"
  %body(class="#{page_classes}")
    %header
      = partial "partials/header"
    %article
      = yield
    %footer
      = partial "partials/footer"
    = javascript_include_tag "all"
