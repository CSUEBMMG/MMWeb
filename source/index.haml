%section.hero-container
  %div
    =image_tag "bg1.jpg"
    .hero-caption.hero-caption-top-left
      %p.hero-caption-title
        = lorem.words(2)
      %p
        = lorem.sentences 3
      %p
        = succeed '...' do
          = link_to 'Read more', '#'
  %div
    =image_tag "bg2.jpg"
    .hero-caption.hero-caption-bottom-left
      %p.hero-caption-title
        = lorem.words(2)
      %p
        = lorem.sentences 3
      %p
        = succeed '...' do
          = link_to 'Continue read', '#'
  %div
    =image_tag "bg3.jpg"
